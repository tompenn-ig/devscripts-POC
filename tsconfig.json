{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20",
  "_version": "16.1.0",
  "compilerOptions": {
    "lib": [
      "es2021"
    ],
    "module": "ES2022",
    "target": "es2021",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node"
  },
  "include": [
    "src/**/*"
  ]
}
